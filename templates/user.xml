<templateSet group="user">
  <template name="forvar" value="for(var $INDEX$ = 0, forLen_$INDEX$ = array.length; $INDEX$ &lt; forLen_$INDEX$; $INDEX$++) {&#10;  var item_$INDEX$ = array[$INDEX$];&#10;}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="INDEX" expression="jsSuggestIndexName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="trycatch" value="try {&#10;  $SELECTION$&#10;} catch (error) {&#10;  &#10;}" description="trycatch" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="switch" value="switch ($VALUE$) {&#10;  case $SELECTION$:&#10;    $END$&#10;    break;&#10;&#10;  default:&#10;    break;&#10;}" description="switch" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
      <option name="Vue" value="true" />
    </context>
  </template>
</templateSet>